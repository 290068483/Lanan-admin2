<script setup name="${businessName}">
import { add${BusinessName}, del${BusinessName}, export${BusinessName}, get${BusinessName}, list${BusinessName}, update${BusinessName} } from "@/api/${moduleName}/${businessName}";
#foreach($column in $columns)
#if($column.dictType != "")
import { $column.dictType } from "@/api/system/dict/data";
#end
#end

#foreach($subTable in $table.subTable)
import { list${subTable.className} } from "@/api/${subTable.moduleName}/${subTable.businessName}";
#end

const { proxy } = getCurrentInstance();
const { ${pkColumn.javaField} } = proxy.useDict(#foreach($column in $columns)#if($column.dictType != "")'$column.dictType'#if($foreach.hasNext),#end#end);

#[[
const ${businessName}List = ref([]);
const loading = ref(true);
const showSearch = ref(true);
const ids = ref([]);
const single = ref(true);
const multiple = ref(true);
const total = ref(0);
const title = ref("");
const dialog = ref(false);
const dateRange = ref([]);
const ${subclassName}List = ref([]);
const checked${subClassName} = ref([]);
]]#

#foreach($column in $columns)
#if($column.htmlType == "summernote")
#[[
let $column.javaField = ref(null);
]]#
#end
#end

#[[
const data = reactive({
  form: {},
  queryParams: {
    pageNum: 1,
    pageSize: 10,
#foreach($column in $columns)
#if($column.queryType != "")
    $column.javaField: undefined,
#end
#end
  },
  rules: {
#foreach($column in $columns)
#if($column.isRequired)
    $column.javaField: [
      { required: true, message: "$column.columnComment不能为空", trigger: "blur" }
    ],
#end
#end
  }
});

const { queryParams, form, rules } = toRefs(data);
]]#

#foreach($subTable in $table.subTable)
#[[
/** ${subTable.functionName}序号 */
function row${subTable.className}Index({ row, rowIndex }) {
  row.index = rowIndex + 1;
}
]]#
#end

#[[
/** 查询${functionName}列表 */
function getList() {
  loading.value = true;
  list${BusinessName}(proxy.addDateRange(queryParams.value, dateRange.value)).then(response => {
    ${businessName}List.value = response.rows;
    total.value = response.total;
    loading.value = false;
  });
}

/** 转换${functionName}数据 */
function handle${BusinessName}Data(data) {
#foreach($column in $columns)
#if($column.htmlType == "checkbox")
  if (data.${column.javaField} && data.${column.javaField}.length > 0) {
    data.${column.javaField} = data.${column.javaField}.join(",");
  }
#end
#end
  return data;
}

/** 取消按钮 */
function cancel() {
  dialog.value = false;
  reset();
}

/** 表单重置 */
function reset() {
  form.value = {
#foreach($column in $columns)
#if($column.htmlType == "checkbox")
    $column.javaField: [],
#elseif($column.htmlType == "radio" || $column.htmlType == "select")
    $column.javaField: undefined,
#else
    $column.javaField: undefined,
#end
#end
  };
#foreach($column in $columns)
#if($column.htmlType == "summernote")
  $column.javaField.value = null;
#end
#end
  ${subclassName}List.value = [];
  checked${subClassName}.value = [];
  proxy.resetForm("${businessName}Ref");
}

/** 搜索按钮操作 */
function handleQuery() {
  queryParams.value.pageNum = 1;
  getList();
}

/** 重置按钮操作 */
function resetQuery() {
  dateRange.value = [];
  proxy.resetForm("queryRef");
  handleQuery();
}

/** 多选框选中数据 */
function handleSelectionChange(selection) {
  ids.value = selection.map(item => item.${pkColumn.javaField});
  single.value = selection.length != 1;
  multiple.value = !selection.length;
}

/** 更多操作 */
function handleMore() {
  proxy.$refs["topRightPanel"].toggle();
}

/** 新增按钮操作 */
function handleAdd() {
  reset();
  dialog.value = true;
  title.value = "添加${functionName}";
}

/** 修改按钮操作 */
function handleUpdate(row) {
  reset();
  const _${pkColumn.javaField} = row.${pkColumn.javaField} || ids.value;
  get${BusinessName}(_${pkColumn.javaField}).then(response => {
    form.value = response.data;
#foreach($column in $columns)
#if($column.htmlType == "checkbox")
    form.value.$column.javaField = response.data.$column.javaField.split(",");
#end
#end
    ${subclassName}List.value = response.data.${subclassName}List;
    dialog.value = true;
    title.value = "修改${functionName}";
  });
}

/** 提交按钮 */
function submitForm() {
  proxy.$refs["${businessName}Ref"].validate(valid => {
    if (valid) {
#foreach ($column in $columns)
#if($column.htmlType == "checkbox")
      form.value.${column.javaField} = form.value.${column.javaField}.join(",");
#end
#end
      form.value.${subclassName}List = ${subclassName}List.value;
      if (form.value.${pkColumn.javaField} != null) {
        update${BusinessName}(form.value).then(response => {
          proxy.$modal.msgSuccess("修改成功");
          dialog.value = false;
          getList();
        });
      } else {
        add${BusinessName}(form.value).then(response => {
          proxy.$modal.msgSuccess("新增成功");
          dialog.value = false;
          getList();
        });
      }
    }
  });
}

/** 删除按钮操作 */
function handleDelete(row) {
  const ${pkColumn.javaField}s = row.${pkColumn.javaField} || ids.value;
  proxy.$modal.confirm('是否确认删除${functionName}编号为"' + ${pkColumn.javaField}s + '"的数据项？').then(() => {
    return del${BusinessName}(${pkColumn.javaField}s);
  }).then(() => {
    getList();
    proxy.$modal.msgSuccess("删除成功");
  }).catch(() => {});
}

/** 导出按钮操作 */
function handleExport() {
  proxy.$modal.confirm("是否确认导出所有${functionName}数据项？").then(() => {
    return export${BusinessName}(queryParams.value);
  }).then(response => {
    proxy.$download.name(response.msg);
  }).catch(() => {});
}

/** ${subTable.functionName}添加按钮操作 */
function handleAdd${subClassName}() {
  let obj = {};
#foreach($subColumn in $subTable.columns)
#if($subColumn.pk)
  obj.${subColumn.javaField} = null;
#elseif($subColumn.htmlType == "select" || $subColumn.htmlType == "radio")
  obj.${subColumn.javaField} = "";
#elseif($subColumn.htmlType == "checkbox")
  obj.${subColumn.javaField} = [];
#elseif($subColumn.htmlType == "datetime")
  obj.${subColumn.javaField} = null;
#else
  obj.${subColumn.javaField} = "";
#end
#end
  ${subclassName}List.value.push(obj);
}

/** ${subTable.functionName}删除按钮操作 */
function handleDelete${subClassName}() {
  if (checked${subClassName}.value.length == 0) {
    proxy.$modal.msgError("请先选择要删除的${subTable.functionName}数据");
  } else {
    const ${subclassName}s = ${subclassName}List.value;
    const checked${subClassName}s = checked${subClassName}.value;
    ${subclassName}List.value = ${subclassName}s.filter(function(item) {
      return checked${subClassName}s.indexOf(item.index) == -1;
    });
  }
}

/** 复选框选中数据 */
function handle${subClassName}SelectionChange(selection) {
  checked${subClassName}.value = selection.map(item => item.index);
}
