            </if>
            <if test="details != null and details != ''">
                details = #{details},
            </if>
        </set>
        where archive_id = #{archiveId}
    </update>
    
    <delete id="deletePendingArchivedById" parameterType="Long">
        delete from pending_archived where archive_id = #{archiveId}
    </delete>
    
    <delete id="deletePendingArchivedByIds" parameterType="Long">
        delete from pending_archived where archive_id in
        <foreach item="archiveId" collection="array" open="(" separator="," close=")">
            #{archiveId}
        </foreach>
    </delete>
</mapper>