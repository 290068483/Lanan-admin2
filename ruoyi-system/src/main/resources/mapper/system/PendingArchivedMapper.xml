            </if>
            <if test="designer != null and designer != ''">
                #{designer},
            </if>
            <if test="installer != null and installer != ''">
                #{installer},
            </if>
            <if test="measurementCount != null">
                #{measurementCount},
            </if>
            <if test="afterSalesCount != null">
                #{afterSalesCount},
            </if>
            <if test="extendedRelationship != null and extendedRelationship != ''">
                #{extendedRelationship},
            </if>
            <if test="details != null and details != ''">
                #{details},
            </if>
        </trim>
    </insert>
    
    <update id="updatePendingArchived" parameterType="PendingArchived">
        update pending_archived
        <set>
            <if test="customId != null">
                custom_id = #{customId},
            </if>
            <if test="customerName != null and customerName != ''">
                customer_name = #{customerName},
            </if>
            <if test="customerSatisfaction != null">
                customer_satisfaction = #{customerSatisfaction},
            </if>
            <if test="orderDate != null">
                order_date = #{orderDate},
            </if>
            <if test="completionDate != null">
                completion_date = #{completionDate},
            </if>
            <if test="totalContractAmount != null">
                total_contract_amount = #{totalContractAmount},
            </if>
            <if test="salesman != null and salesman != ''">
                salesman = #{salesman},
            </if>
            <if test="designer != null and designer != ''">
                designer = #{designer},
            </if>
            <if test="installer != null and installer != ''">
                installer = #{installer},
            </if>
            <if test="measurementCount != null">
                measurement_count = #{measurementCount},
            </if>
            <if test="afterSalesCount != null">
